{
    "manifest_version": 2,
    "name": "no-ai-search",
    "version": "1.0",
    "description": "Appends '&udm=14' to end of Google search URLs",
    "icons": {
        "48": "icons/no-ai-48.png"
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "history",
        "*://www.google.com/*"
    ],
    "background": {
        "scripts": [
            "no-ai-search.js"
        ],
        "persistent": true
    }
}